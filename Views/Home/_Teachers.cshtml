        <thead>
            <tr class="table bg-secondary">
                <th>Teacher Id</th>
                <th>Name</th>
                <th>Qualification</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var teacher in Model)
            {
                <tr>
                    <td>@teacher.TeacherId</td>
                    <td>@teacher.Name</td>
