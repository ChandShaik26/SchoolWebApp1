@model IList<WebApplication1.Entities.Class>
@if (Model != null && Model.Count > 0)
{
    <table class="table table-striped table-bordered">
        <thead>
            <tr class="table bg-secondary">
                <th>Class Id</th>
                <th>Name</th>
                <th>Standard</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var classRoom in Model)
            {
                <tr>
                    <td>@classRoom.ClassId</td>
                    <td>@classRoom.ClassName</td>
                    <td>@classRoom.Standard</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <h2>No Classes ..</h2>
}